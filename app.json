{
  "expo": {
    "name": "Cakto",
    "slug": "cakto",
    "version": "1.0.0",
    "newArchEnabled": true,
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-font"],
    "orientation": "portrait",
    "icon": "src/assets/images/cakto-image.png",
    "backgroundColor": "rgba(0, 161, 104, 1)",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/images/cakto-image.png",
      "resizeMode": "contain",
      "backgroundColor": "rgba(0, 161, 104, 1)"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/cakto-image.png",
        "backgroundColor": "rgba(0, 161, 104, 1)"
      },
      "package": "com.cakto.production"
    }
  }
}
