{
  "expo": {
    "name": "Cakto",
    "slug": "cakto",
    "version": "1.0.0",
    "newArchEnabled": true,
    "web": {
      "favicon": "src/assets/images/cakto.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-font"],
    "orientation": "portrait",
    "icon": "src/assets/images/cakto.png",
    "backgroundColor": "#00a168",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/images/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a168"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cakto.production"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/splash-screen.png",
        "backgroundColor": "#00a168"
      },
      "package": "com.cakto.production"
    }
  }
}
